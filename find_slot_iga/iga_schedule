from selenium import webdriver
from selenium.webdriver.common.by import By
from selenium.webdriver.common.keys import Keys
import time

# Set up the WebDriver (you may need to download the appropriate driver for your browser)
driver = webdriver.Chrome()  # Or use `webdriver.Firefox()` for Firefox

# Open the website
website_url = "https://book.stadeiga.com/courtbooking/home/calendarDayView.do?id=16&iYear=2024&iMonth=11&iDate=30"  # Replace with the URL of the login page
driver.get(website_url)

# Locate the username and password input fields
username_field = driver.find_element(By.NAME, "username")  # Replace "username" with the actual name/id/class of the input field
password_field = driver.find_element(By.NAME, "password")  # Replace "password" with the actual name/id/class of the input field

# Enter login credentials
username = "your_username"
password = "your_password"

username_field.send_keys(username)
password_field.send_keys(password)

# Simulate pressing the login button
login_button = driver.find_element(By.NAME, "login")  # Replace "login" with the actual name/id/class of the login button
login_button.click()

# Wait for a while to observe the behavior
time.sleep(5)

# Close the browser
driver.quit()

# https://book.stadeiga.com/courtbooking/home/login.do